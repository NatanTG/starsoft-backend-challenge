    "typeorm:revert": "typeorm-ts-node-commonjs migration:revert -d src/core/config/typeorm.config.ts",
    "test": "NODE_OPTIONS='--expose-gc --max-old-space-size=4096' jest --runInBand",
    "test:watch": "NODE_OPTIONS='--expose-gc --max-old-space-size=4096' jest --watch --runInBand",
    "test:cov": "NODE_OPTIONS='--expose-gc --max-old-space-size=4096' jest --coverage --runInBand",
